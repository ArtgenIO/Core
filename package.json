{
  "name": "@artgenio/core",
  "version": "0.5.0",
  "description": "Next generation, Headless CMS with No Code engine",
  "keywords": [
    "headless-cms",
    "headless",
    "nocode",
    "lowcode",
    "database manager",
    "cms",
    "headless",
    "react"
  ],
  "main": "build/main.js",
  "homepage": "https://artgen.io",
  "license": "GPL-3.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artgenio/core.git"
  },
  "bugs": {
    "url": "https://github.com/artgenio/core/issues"
  },
  "author": {
    "name": "Varga Zsolt",
    "email": "hello@hisorange.me",
    "url": "https://hisorange.me"
  },
  "engines": {
    "node": ">=12.x",
    "yarn": "1.x"
  },
  "scripts": {
    "build:docker": "docker build . --target=production -t artgenio/core:dev",
    "build:api": "tsc -p tsconfig.prod.json",
    "build:ui": "vite build --config ./src/modules/admin/vite.config.js",
    "build:test": "CYPRESS_COVERAGE=1 yarn build",
    "build": "yarn build:api && yarn build:ui",
    "dev": "nodemon -I --exec node --experimental-specifier-resolution=node --loader ts-node/esm --no-warnings ./src/main.ts | pino-pretty",
    "start": "NODE_ENV=production node --experimental-specifier-resolution=node build/main.js",
    "test": "yarn test:type && yarn test:api && yarn build:test && yarn test:ui && yarn test:merge",
    "test:type": "tsc -p tsconfig.prod.json --noEmit",
    "test:api": "node --experimental-vm-modules --no-warnings ./node_modules/.bin/jest -i --no-cache",
    "test:ui": "ARTGEN_DEMO=1 start-test 'NODE_ENV=production nyc --silent node --experimental-specifier-resolution=node build/main.js' http-get://127.0.0.1:7200/admin 'cypress run'",
    "test:merge": "nyc report",
    "test:cy:dev": "start-test 'yarn dev' http-get://127.0.0.1:7200/admin 'cypress run'",
    "test:api:dev": "jest -i --collectCoverage=0",
    "test:api:debug": "node --inspect --inspect-brk node_modules/.bin/jest -i --collectCoverage=0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "artgen": "./bin/artgen"
  },
